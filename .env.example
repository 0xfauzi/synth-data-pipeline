# API Keys (at least one required)
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...

# Optional: Model Configuration
DEFAULT_GENERATOR_MODEL=gpt-4
DEFAULT_JUDGE_MODEL=gpt-4
DEFAULT_TEMPERATURE_GENERATE=0.9
DEFAULT_TEMPERATURE_JUDGE=0.2

# Optional: Processing Configuration
MAX_WORKERS=4
BATCH_SIZE=50
RETRY_ATTEMPTS=3
RETRY_DELAY=1.0

# Optional: Local Model Configuration (for Outlines)
# LOCAL_MODEL_PATH=/path/to/model
# DEVICE=cuda  # or cpu
